#include<stdio.h>
#include<math.h>
int main()
{
   int n,arr[20],i;
   printf("\nEnter the number of elements:");
   scanf("%d",&n);
   printf("\nEnter the elements of the array:");
   for(i=0;i<n;i++)
     scanf("%d",&arr[i]);
   insertionsort(arr);
}
insertionsort(int *x)
{
   int temp,i,j;
   for(i=1;i<n;i++)
   {
     temp=x[i];
     j=i-1;
     while(temp<x[j] && j>=0)
     {
        x[j+1]=x[j];
        j=j-1;
     }
     x[j+1]=temp;
  }
  return x;
}
